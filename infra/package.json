{
  "name": "infra",
  "version": "0.1.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "synth": "npm run build && cdk synth",
    "build-code": "cd ../code && npm install && npm run build",
    "deploy": "npm run build-code && npm run build && cdk deploy --profile aws-dev",
    "destroy": "cdk destroy"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.165.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-cdk/aws-amplify": "^1.203.0",
    "@aws-cdk/aws-cloudfront": "^1.203.0",
    "@aws-cdk/aws-cloudfront-origins": "^1.203.0",
    "@aws-cdk/aws-s3": "^1.203.0",
    "@aws-cdk/aws-s3-deployment": "^1.203.0",
    "aws-cdk-lib": "2.165.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
